{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.js"
        }
    ],
    "env": {
        "DATABASE_URL": "@database_url",
        "GROQ_API_KEY": "@groq_api_key"
    }
}